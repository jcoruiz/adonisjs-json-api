{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
     "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName"      : "API",
        "footer"          : "",
        "copyright"       : "The One Hundred Monkeys",
        "navType"         : "vertical",
        "theme"           : "spacelab",
        "linenums"        : true,
        "collapseSymbols" : false,
        "inverseNav"      : true
    }
}
